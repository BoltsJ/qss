<div id="{{id}}" style="top: {{top}}; left: {{left}}; transform: scale({{scale}}); transform-origin: top left">
    <div id="qss-hudTitle">Quick Status Select</div>

    <div id="qss-reposition"><i class="fas fa-arrows-alt"></i></div>
    <div class="qss-category" id="qss-category-{{this.id}}">

        <div class="qss-content">
            <input class="qss-quick-input" type="search"></input>
        </div>
    </div>
    <ul>
        {{#each this.statuses}}
        <li>{{this.label}} <img src="{{this.icon}}" class="qss-status-icon" /> </li>
        {{/each}}
    </ul>
</div>